<openerp>
    <data>
    
        <!-- INHERITED VIEW FOR THE OBJECT : file_document -->

        <record id="file_document_view_form" model="ir.ui.view">
            <field name="name">account_statement_atos.file_document.view_form</field>
            <field name="model">file.document</field>
            <field name="inherit_id" ref="module_name_to_inherit.view_id" />
            <field eval="16" name="priority"/>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <data>
                    <field name="field_reference" position="after">
                    </field>
                </data>
            </field>
        </record>

        <record id="file_document_view_tree" model="ir.ui.view">
            <field name="name">account_statement_atos.file_document.view_tree</field>
            <field name="model">file.document</field>
            <field name="inherit_id" ref="module_name_to_inherit.view_id" />
            <field eval="16" name="priority"/>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <data>
                    <field name="field_reference" position="after">
                    </field>
                </data>
            </field>
        </record>
    




    </data>
</openerp>
